{
  "name": "@kovalevskayaschool/pavetra",
  "version": "0.0.1",
  "description": "ui kit for school",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "rollup -c ./rollup.config.mjs && npm run build:css",
    "package": "npm run clean && npm run build && npm run build:css",
    "prepublishOnly": "npm run package",
    "build:vite": "npm run clean && vite build && npm run build:css",
    "build:es": "node ./build.mjs",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.build.json",
    "build:css": "lightningcss --minify --bundle --targets '>= 0.25%' ./src/themes/default/root.css -o ./dist/root.css",
    "sb": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "url": "git://github.com/KovalevskayaSchool/pavetra-ui.git"
  },
  "author": "Aleksei <symblight@gmail.com>",
  "license": "ISC",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./dist/root.css": "./dist/root.css",
    "./dist/bundle.css": "./dist/bundle.css"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "@kovalevskayaschool/pavetra-icons": "github:KovalevskayaSchool/pavetra-icons",
    "@popperjs/core": "^2.11.7",
    "classnames": "^2.3.2",
    "react-aria": "^3.24.0",
    "react-popper": "^2.3.0",
    "date-fns": "^2.29.3",
    "react-stately": "^3.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@react-types/shared": "^3.18.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/builder-vite": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-vite": "^7.0.2",
    "@storybook/testing-library": "^0.1.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "esbuild": "^0.17.16",
    "esbuild-css-modules-plugin": "^2.7.1",
    "fast-glob": "^3.2.12",
    "lightningcss": "^1.19.0",
    "lightningcss-cli": "^1.19.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-import-css": "^3.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "storybook": "^7.0.2",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.2.0",
    "vite-plugin-lightningcss": "^0.0.5"
  }
}
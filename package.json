{
  "name": "@kovalevskayaschool/pavetra",
  "version": "0.0.30",
  "description": "ui kit for school",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "cross-env NODE_ENV=production npm-run-all clean build:*",
    "build:rollup": " rollup -c ./rollup.config.mjs",
    "build:css": "postcss ./src/themes/default/root.css --config ./postcss.config.cjs  -o ./dist/root.css -m",
    "package": "npm run build",
    "prepublishOnly": "npm run package",
    "sb": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier --write --cache .",
    "prepare": "husky install"
  },
  "repository": {
    "url": "git://github.com/KovalevskayaSchool/pavetra-ui.git"
  },
  "author": "Aleksei <symblight@gmail.com>",
  "license": "ISC",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./dist/root.css": "./dist/root.css",
    "./dist/bundle.css": "./dist/bundle.css"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "pnpm run format"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "@internationalized/date": "^3.4.0",
    "@kovalevskayaschool/pavetra-icons": "github:KovalevskayaSchool/pavetra-icons",
    "@popperjs/core": "^2.11.8",
    "classnames": "^2.3.2",
    "react-aria": "^3.27.0",
    "react-popper": "^2.3.0",
    "react-stately": "^3.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/plugin-syntax-import-assertions": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@csstools/postcss-oklab-function": "^3.0.1",
    "@react-types/shared": "^3.18.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/addon-interactions": "^7.1.1",
    "@storybook/addon-links": "^7.1.1",
    "@storybook/blocks": "^7.1.1",
    "@storybook/builder-vite": "^7.1.1",
    "@storybook/react": "^7.1.1",
    "@storybook/react-vite": "^7.1.1",
    "@storybook/testing-library": "^0.2.0",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "cssnano-preset-lite": "^3.0.0",
    "date-fns": "^2.30.0",
    "esbuild": "^0.18.17",
    "esbuild-css-modules-plugin": "^2.7.1",
    "fast-glob": "^3.3.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.26.3",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-import-css": "^3.3.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^7.1.1",
    "storybook-css-modules": "^1.0.8",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-plugin-dts": "^3.3.1"
  }
}
